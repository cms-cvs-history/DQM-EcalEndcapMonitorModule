#!/bin/sh

eval `scramv1 runtime -sh`

if [ "$SCRAM_ARCH" = "slc4_ia32_gcc345" ]; then
 export LD_LIBRARY_PATH=`echo $LD_LIBRARY_PATH | sed -e 's;/cms/software/slc3_ia32_gcc323/external/gcc/3.2.3/lib:;;'`
fi  

node="localhost"
port="9090"

if [ "$1" !=  "" ]; then
  node=$1
fi

if [ "$2" != "" ]; then
  port=$2
fi

iguana --dqm-collector-hostname $node --dqm-collector-port $port --iguana-session 'Vis Example--NTuple browser'
